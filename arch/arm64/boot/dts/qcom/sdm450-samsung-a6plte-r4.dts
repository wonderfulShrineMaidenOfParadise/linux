&tlmm {
	/* Fingerprint sensor at SPI4 (BLSP1 QUP4) */
	gpio-reserved-ranges = <12 4>;

	accel_int_default: accel-int-default-state {
		pins = "gpio42";
		function = "gpio";
		bias-disable;
	};

	charger_int_default: charger-int-state {
		pins = "gpio60";
		function = "gpio";
		bias-disable;
	};

	fg_int_default: fg-int-default-state {
		pins = "gpio62";
		function = "gpio";
		bias-disable;
	};

	jack_default: jack-default-state {
		pins = "gpio48";
		function = "gpio";
		bias-disable;
	};

	muic_int_default: muic-int-default-state {
		pins = "gpio63";
		function = "gpio";
		bias-disable;
	};

	quat_tlmm_default: quat-tlmm-state {
		pins = "gpio135", "gpio136", "gpio137", "gpio138";
		function = "sec_mi2s";
		drive-strength = <8>;
		bias-disable;
	};

	ts_int_default: ts-int-default-state {
		pins = "gpio59";
		function = "gpio";
		bias-disable;
	};
};

